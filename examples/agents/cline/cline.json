{
  "$schema": "https://soulrecall.cloud/schemas/cline-config-v1.json",
  "name": "example-cline",
  "type": "cline",
  "description": "Example Cline agent for SoulRecall",
  "version": "1.0.0",
  "mode": "auto",
  "claudeVersion": "3.5",
  "workingDirectory": ".",
  "autoConfirm": false,
  "useReadline": true,
  "allowedCommands": [
    "git",
    "npm",
    "node",
    "ls",
    "cat",
    "mkdir",
    "rm",
    "cp",
    "mv",
    "grep"
  ]
}
